.itemContainer {
  display: flex;
  flex-direction: row;
  height: 60rpx;
}

.itemContent {
  flex: 1;
  display: flex;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  padding-left: 10rpx;
  color: #000;
  font-size: 18px;
}

.itemContent-Finish {
  flex: 1;
  display: flex;
  padding-left: 10rpx;
  color: #000;
  font-size: 18px;
  text-decoration: line-through;
}
